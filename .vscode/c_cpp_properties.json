{
    "configurations": [
        {
            "name": "i686-elf",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/toolchain/x86_64-elf/lib/gcc/x86_64-elf/15.1.0/include/*",
                "${workspaceFolder}/src/kernel",
                "/usr/include/efi",
                "/usr/include/efi/x86_64"
            ],
            "defines": [
                "__KERNEL__",
                "__x86_64__",
                "__FREESTANDING__"
            ],
            "compilerPath": "${workspaceFolder}/toolchain/x86_64-elf/bin/x86_64-elf-gcc",
            "cStandard": "c23",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-std=c11",
                "-g",
                "-ffrestanding",
                "-nostdlib",
                "-fbuiltin",
                "-I."
            ]
        }
    ],
    "version": 4
}